{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "service-3"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "registerWithEureka": true,
      "fetchRegistry": true,
      "validateCerts": false
    },
    "instance": {
      "preferIpAddress": true,
      "ipAddress": "localhost",
      "port": 7282
    }
  }
}
